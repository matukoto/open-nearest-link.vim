{
  "deno.enable": true,
  "deno.unstable": true,
  "deno.config": "./deno.jsonc",
  "deno.importMap": "./import_map.json",
  "deno.lint": true,
  "deno.testing.enable": true,
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://jsr.io": true
  },
  "deno.lint.rules": {
    "tags": ["recommended"],
    "exclude": ["no-unused-vars"]
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
